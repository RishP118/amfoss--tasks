{
    "name": "Weather App",
    "description": "Extension for weather",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "fetch.html",
        "icons": {
            "16": "icon_16.png", 
            "48": "icon_48.png",
            "128": "icon_128.png"
        }
    
    },
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"

    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["fetch.js"]
        }
    ]

}
